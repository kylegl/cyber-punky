  "tokenColors": [
    {
      "name": "HTML Text",
      "scope": [
        "meta.tag",
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Object Keys",
      "scope": [
        "meta.object-literal.key",
        "variable.object.property"
      ],
      "settings": {
        "foreground": "#EA9800"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#00FF9F90",
      }
    },
    {
      "name": "Comment Punctuation",
      "scope": [
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00FF9F90",
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#EA00D9"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
